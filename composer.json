{
  "type": "project",
  "name": "nextbasket/dropshipping-manager",
  "license": "MIT",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "git",
      "url": "https://bitbucket.org/next-basket/process-pool-bundle.git"
    }
  ],
  "require": {
    "php": ">=8.1",
    "ext-simplexml": "*",
    "defuse/php-encryption": "^2.4",
    "doctrine/annotations": "^2.0",
    "doctrine/doctrine-bundle": "^2.12",
    "doctrine/doctrine-migrations-bundle": "^3.3",
    "doctrine/orm": "^2.10",
    "dragonmantank/cron-expression": "^3.3",
    "google/cloud-logging": "^1.28",
    "nextbasket/process-pool-bundle": "^3.0",
    "scienta/doctrine-json-functions": "^6.2",
    "stof/doctrine-extensions-bundle": "^1.8",
    "symfony/amqp-messenger": "6.4.*",
    "symfony/cache": "6.4.*",
    "symfony/console": "6.4.*",
    "symfony/doctrine-messenger": "6.4.*",
    "symfony/dotenv": "6.4.*",
    "symfony/flex": "^2",
    "symfony/framework-bundle": "6.4.*",
    "symfony/http-client": "6.4.*",
    "symfony/intl": "6.4.*",
    "symfony/messenger": "6.4.*",
    "symfony/monolog-bundle": "^3.10",
    "symfony/process": "6.4.*",
    "symfony/runtime": "6.4.*",
    "symfony/scheduler": "6.4.*",
    "symfony/security-bundle": "6.4.*",
    "symfony/serializer": "6.4.*",
    "symfony/translation": "6.4.*",
    "symfony/uid": "6.4.*",
    "symfony/validator": "6.4.*",
    "symfony/yaml": "6.4.*",
    "zircote/swagger-php": "^4.8"
  },
  "config": {
    "allow-plugins": {
      "symfony/flex": true,
      "symfony/runtime": true,
      "phpstan/extension-installer": true
    },
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php73": "*",
    "symfony/polyfill-php74": "*",
    "symfony/polyfill-php80": "*",
    "symfony/polyfill-php81": "*"
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "6.4.*"
    },
    "phpstan": {
      "includes": [
        "extension.neon"
      ]
    }
  },
  "require-dev": {
    "coduo/php-matcher": "^6.0",
    "dama/doctrine-test-bundle": "^7.3",
    "dg/bypass-finals": "^1.6",
    "doctrine/doctrine-fixtures-bundle": "^3.5",
    "madewithlove/license-checker": "^1.6",
    "phpstan/extension-installer": "^1.3",
    "phpstan/phpstan": "^1.10",
    "phpstan/phpstan-doctrine": "^1.3",
    "phpstan/phpstan-symfony": "^1.3",
    "phpunit/phpunit": "^9.5",
    "symfony/browser-kit": "6.4.*",
    "symfony/css-selector": "6.4.*",
    "symfony/maker-bundle": "^1.38",
    "symfony/phpunit-bridge": "^7.0",
    "symfony/web-profiler-bundle": "6.4.*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    }
  }
}
